# Production Environment Configuration
# Copy this file to .env.production for production builds

# canonical URL of the application (production)
NEXTAUTH_URL=https://your-domain.com

# secret used to encrypt JWT and hash email verification token
# IMPORTANT: Generate a UNIQUE secret for production with: openssl rand -base64 32
# DO NOT use the same secret across different environments
NEXTAUTH_SECRET=CHANGE_ME_GENERATE_UNIQUE_SECRET_FOR_PRODUCTION
